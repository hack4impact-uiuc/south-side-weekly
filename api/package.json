{
  "name": "mern-template-api",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
    "start:prod": "node -r dotenv/config ./bin/www",
    "start:dev": "nodemon -r dotenv/config ./bin/www",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint --fix src --ext ts",
    "format": "prettier --write \"./**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"./**/*.{ts,json,md}\"",
    "test": "yarn build && jest --setupFiles dotenv/config",
    "build": "tsc"
  },
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "if-env": "^1.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.11.13",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.9",
    "@types/node": "^14.14.26",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "typescript": "^4.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
