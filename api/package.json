{
  "name": "ssw-contributor-hub-api",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
    "start:prod": "node -r dotenv/config ./bin/www",
    "start:dev": "nodemon --watch src --ext ts,js,json --exec \"tsc --incremental && node -r dotenv/config ./bin/www\"",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint --fix src --ext ts",
    "format": "prettier --write \"./**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"./**/*.{ts,json,md}\"",
    "test": "yarn build && jest --forceExit --detectOpenHandles --setupFiles dotenv/config",
    "build": "tsc"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.0.2",
    "@types/cookie-session": "^2.0.42",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.171",
    "@types/mongodb": "^3.6.8",
    "@types/mongoose-lean-virtuals": "^0.5.2",
    "@types/morgan": "^1.9.2",
    "@types/node-cron": "^2.0.4",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "if-env": "^1.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.11.13",
    "mongoose-lean-virtuals": "^0.9.0",
    "morgan": "~1.10.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "swagger-jsdoc": "^6.1.0"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.9",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ssw-common": "file:../common",
    "supertest": "^6.1.1",
    "swagger-ui-express": "^4.3.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
